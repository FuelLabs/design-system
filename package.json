{
  "name": "@fuels-ui/design-system",
  "version": "0.0.1",
  "private": true,
  "description": "Fuel Design System",
  "author": "Fuel Labs <contact@fuels-ui.sh> (https://fuel.network/)",
  "engines": {
    "node": ">=14.19.0",
    "pnpm": ">=7"
  },
  "homepage": "https://github.com/FuelLabs/design-sytem#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FuelLabs/design-sytem.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/FuelLabs/design-sytem/issues"
  },
  "workspaces": [
    "./apps/**/*",
    "./common/*",
    "./design-system/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:apps": "turbo run build --filter=./apps/**",
    "build:libs": "turbo run build --filter=!./apps/**",
    "build:libs-watch": "pnpm -r --filter=!./apps/** --parallel build --watch",
    "dev": "turbo run dev --parallel",
    "dev:apps": "turbo run dev --parallel --filter=./apps/**",
    "dev:libs": "turbo run dev --parallel --filter=!./apps/**",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "pnpm lint:check --fix",
    "lint": "run-s lint:check prettier:check",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "test": "turbo run test -- --passWithNoTests"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@fuels-ui/test-utils": "workspace:*",
    "@jest/types": "^28.1.0",
    "@swc/core": "^1.2.179",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "tsup": "^5.12.7",
    "turbo": "^1.2.8",
    "typescript": "^4.6.4"
  }
}
