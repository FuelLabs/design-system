{
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"],
      "cache": true
    },
    "build:storybook": {
      "outputs": ["storybook-static/**"],
      "dependsOn": ["build"],
      "cache": true
    },
    "test": {
      "dependsOn": []
    },
    "dev": {
      "cache": false
    },
    "//#lint": {
      "cache": true
    }
  }
}
