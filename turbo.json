{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "build:storybook": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test": {
      "dependsOn": ["ts:check"]
    },
    "dev": {
      "cache": false
    },
    "ts:check": {
      "cache": true
    }
  }
}
